package helper;

import dAO.CheckUserDAO;
import dTO.LoginDTO;

public class LoginHelper {
	public boolean checkUser(LoginDTO loginDTO)
	{
		boolean result=false;
		CheckUserDAO checkUserDAO=new CheckUserDAO();
		result=checkUserDAO.checkUserLogin(loginDTO);
		return result;
	}
	
	public boolean setLastLogin()
	{
		boolean result=false;
		
		
		return result;
	}
	
	
	public boolean returnUserDTO()
	{
		boolean result=false;
		
		return result;
		
	}
	
	
}
