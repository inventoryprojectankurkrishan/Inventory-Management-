package helper;

import java.sql.SQLException;

import dAO.CheckUserDAO;
import dTO.LoginDTO;

public class LoginHelper {
	public boolean checkUser(LoginDTO loginDTO)
	{
		boolean result=false;
		CheckUserDAO checkUserDAO=new CheckUserDAO();
		
		try 
		{
			result=checkUserDAO.checkUserLogin(loginDTO);
		}
		
		catch (SQLException e)
		{
			e.printStackTrace();
			System.out.println("SQL Exception generated in checkUser in LoginHelper");
		}
		return result;
	}
	
	public boolean setLastLogin()
	{
		boolean result=false;
		
		
		return result;
	}
	
	
	public boolean returnUserDTO()
	{
		boolean result=false;
		
		return result;
		
	}
	
	
}
